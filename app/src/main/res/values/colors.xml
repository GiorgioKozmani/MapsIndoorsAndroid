<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- === App's branding colors === -->
	<!-- Color for the app bar -->
	<color name="colorPrimary">#B02A1A</color>

	<!-- Darker variant of the above for the status bar and contextual app bars -->
	<color name="colorPrimaryDark">#7B1D12</color>

	<!-- Theme UI controls like checkboxes and text fields -->
	<color name="colorAccent">#333333</color>

	<!-- Splash screen background -->
	<color name="splashScreenBackgroundColor">@color/colorPrimary</color>

	<!-- Splash screen text -->
	<color name="splashScreenTextColor">@color/white</color>

	<color name="secondaryButtonColor">#d1d1d1</color>
	<color name="blueGray">#43aaa0</color>
	<color name="hotPink">#EF6CCE</color>

	<color name="light_grey">#ededed</color>

	<color name="info_window_text_color">@color/blueGray</color>

	<color name="building_outline_color">@color/hotPink</color>

	<color name="yellow">#FFC107</color>
	<color name="directionLegColor">#3071D9</color>



	<color name="floorselector_background_btn_default">@android:color/white</color>
	<color name="floorselector_background_btn_selected">@color/secondaryButtonColor</color>

</resources>